calculatePvalues <- function(m,n,mu,nu,sd,B){
p=numeric(B)
for (b in 1:B) {
x=rnorm(m,mu,sd)
y=rnorm(n,nu,sd)
p[b]=t.test(x,y,var.equal=TRUE)[[3]]
}
return(p)
}
