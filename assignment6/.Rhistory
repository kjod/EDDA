calculatePvaluesNus <- function(m,n,mu,nus,sd,B){
powers=numeric(length(nus))
for (i in 1:length(nus)) {
nu=nus[i]
p=calculatePvalues(m,n,mu,nu,sd,B)
powers[i]=mean(p<0.05)
}
return(powers)
}
