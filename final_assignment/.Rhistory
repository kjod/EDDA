permutation_test = function(mystat, col1, col2){
B=1000
tstar=numeric(B)
for (i in 1:B){
temp=t(apply(cbind(col1,col2),1,sample))
tstar[i]=mystat(temp[,1],temp[,2])
}
myt=mystat(col1,col2)
pl=sum(tstar<myt)/B
pr=sum(tstar>myt)/B
p=2*min(pl,pr)
p
}
